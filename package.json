{
  "name": "d-merge",
  "description": "patch",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/SARDONYX-sard/d-merge",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf ./gui/frontend/out && tauri build",
    "build:front": "next build ./gui/frontend",
    "build:icons": "cargo tauri icon ./gui/backend/icons/icon.png",
    "dev": "tauri dev",
    "dev:front": "next ./gui/frontend",
    "fmt": "biome format ./ && prettier --cache --write \"**/*.{yaml,yml}\" --ignore-path ./.gitignore && cargo fmt --all",
    "lint": "biome lint ./ && cargo clippy --workspace",
    "lint:fix": "npm run fmt && biome check --write ./  && cargo clippy --workspace --fix --allow-staged --allow-dirty",
    "tauri": "tauri",
    "test": "vitest run",
    "test:all": "npm test && npm run test:back",
    "test:back": " cargo test --workspace",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/lab": "6.0.0-beta.13",
    "@mui/material": "^6.1.5",
    "@mui/x-data-grid": "^7.22.0",
    "@tauri-apps/api": "^2.0.3",
    "@tauri-apps/plugin-dialog": "^2.0.1",
    "@tauri-apps/plugin-fs": "^2.0.1",
    "@tauri-apps/plugin-shell": "^2.0.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-8a03594-20241020",
    "i18next": "^23.16.4",
    "next": "15.0.1",
    "notistack": "^3.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.1",
    "react-i18next": "^15.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tauri-apps/cli": "^2.0.4",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "16.0.1",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react-swc": "3.7.1",
    "eslint": "9.13.0",
    "eslint-config-next": "15.0.1",
    "jsdom": "25.0.1",
    "prettier": "^3",
    "rimraf": "^6",
    "typescript": "5.6.3",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.3"
  },
  "overrides": {
    "monaco-editor": "^0.49.0"
  },
  "packageManager": "npm@10.8.3"
}
